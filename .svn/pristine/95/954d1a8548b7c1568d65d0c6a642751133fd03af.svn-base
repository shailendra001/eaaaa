#pragma mark UITableViewDelegate methods

 - (NSString *)tableView:(UITableView *)tableView titleForHeaderInSection:(NSInteger)section{
         NSString *header  = [arrHeaderTitle objectAtIndex:section];
 
         return header;
 }
 
 - (UIView *)tableView:(UITableView *)tableView viewForHeaderInSection:(NSInteger)section {
 
         //Header View
         UIView *headerView = [[UIView alloc] init];
         headerView.backgroundColor=[UIColor whiteColor];
         
         //Header Title
         UILabel *myLabel = [[UILabel alloc] init];
         myLabel.frame = CGRectMake(8, 8, tableView.frame.size.width-16, 20);
         myLabel.font=[UIFont fontWithName:FONT_DOSIS_REGULAR size:15];
         myLabel.text = [self tableView:tableView titleForHeaderInSection:section];
         myLabel.textAlignment=NSTextAlignmentCenter;
         myLabel.textColor=(section==0) ? [UIColor blackColor] : [UIColor redColor];
         myLabel.backgroundColor=[UIColor clearColor];
         [headerView addSubview:myLabel];
         
         
         return headerView;
 }
 
 - (CGFloat)tableView:(UITableView *)tableView heightForHeaderInSection:(NSInteger)section{
         NSInteger height=36;
         if(section==2 || [[EAGallery sharedClass] roleType]==BecomeAnArtistPending) height=0;
         return height;
 }


